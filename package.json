{
  "name": "tombik",
  "version": "1.0.0",
  "main": "dist-electron/main/index.js",
  "description": "Cross platform pomodoro and micro task/timer management app",
  "license": "MIT",
  "private": true,
  "keywords": [
    "electron",
    "rollup",
    "vite",
    "vue3",
    "vue",
    "pomodoro",
    "timer"
  ],
  "debug": {
